keywords = {Heterochromatin, Gene Expression Regulation: Developmental, Inversion: Chromosome, Animals, Chromosomes, Conserved Sequence, Base Sequence, Phylogeny, Embryo: Nonmammalian, Drosophila, Gene Rearrangement, Linkage (Genetics), Genome: Insect, Chromosome Breakage, Repetitive Sequences: Nucleic Acid, Synteny}, 
date-added = {2009-06-08 21:20:52 +0200},
