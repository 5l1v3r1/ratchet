language = {eng},
keywords = {Gene Expression Regulation: Bacterial, Operon, Computational Biology, Computer Simulation, Escherichia coli Proteins, Models: Biological, Membrane Proteins, Gene Order, Escherichia coli K12, Protein Biosynthesis, Bacterial Proteins, Chemotaxis}, 
date-added = {2010-05-07 02:53:02 +0200},
