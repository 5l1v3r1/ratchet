year = {2010},
month = {Jan},
language = {eng},
keywords = {HIV-1, Humans, Time Factors, Epitopes, Immune Evasion, Selection: Genetic, Sequence Analysis: DNA, HIV Infections, Mutation, Genome: Viral, Consensus Sequence, Evolution: Molecular}, 
date-added = {2011-04-17 14:43:22 +0200},
