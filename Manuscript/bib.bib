pages = {11--45},
volume = {128},
year = {1987},
month = {Sep},
language = {eng},
keywords = {DNA, Environment, Gene Expression Regulation, Adaptation: Biological, Mutation, Models: Genetic, Probability, Base Sequence, Biological Clocks, Evolution, Peptides, Amino Acid Sequence, Animals, Antibody Formation}, 
date-added = {2008-08-11 20:29:46 +0200},
