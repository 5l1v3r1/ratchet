pii = {0812664106},
pmid = {19380721},
local-url = {file://localhost/Users/richard/Papers/2009/Lincoln/Lincoln_Proc%20Natl%20Acad%20Sci%20USA_2009.pdf},
uri = {papers://C4A19094-F88E-460C-9862-A50496958974/Paper/p23536},
